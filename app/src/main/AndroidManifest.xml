<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ta.contactmanager" >

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ta.mydictionary.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ta.mydictionary.AddLyricsActivity"
            android:label="@string/title_activity_add_word" >
        </activity>
        <activity
            android:name="ta.mydictionary.EditLyricActivity"
            android:label="@string/title_activity_edit_word" >
        </activity>
        <activity
            android:name="ta.mydictionary.ReadLyrics"
            android:label="" >
        </activity>
    </application>

</manifest>
